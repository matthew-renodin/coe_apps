###############################################################################
# libinit - Kconfig
#
#
###############################################################################

config LIB_INIT
    bool "libinit"
    depends on HAVE_LIB_SEL4 && HAVE_LIBC && HAVE_LIB_SEL4_VKA && HAVE_LIB_SEL4_ALLOCMAN
    select HAVE_SEL4_LIBS
    default y
    help
        A library to help a process initialize.


